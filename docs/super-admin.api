			}


admin/add_user
    post
        summary:
            Sends an email to the specified Email ID for profile creation
        request
            accountToken:<>
            sessionToken:<>
            role:<hr/sales>
            email:<email-address>@therapyondemand.io
            firstName:<>
            lastName:<>
            phone:<>
            callbackUrl:<Absolute URL to which the api should redirect> // Note the url should not end with "/"
        Description:
            email : Email ID should end with @therapyondemand.io
        response
            200
            {
                message:"Email Successfully sent to the user".
            }

